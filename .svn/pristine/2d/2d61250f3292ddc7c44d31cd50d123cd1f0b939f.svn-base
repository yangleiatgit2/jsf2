package cn.uce.suc.app.service;

import javax.annotation.Resource;

import org.springframework.stereotype.Service;

import cn.uce.base.page.Page;
import cn.uce.base.page.Pagination;
import cn.uce.suc.app.entity.AppFeedback;
import cn.uce.suc.app.vo.AppFeedbackVo;
import cn.uce.suc.app.dao.IAppFeedbackDao;

/**
 * @Description: Service类
 * @author litianjian
 * @date 2017年11月29日 下午03:19:08
 */
@Service("appFeedbackService")
public class AppFeedbackService {
	
	/**
	 * 注入Dao
	 */
	@Resource
	private IAppFeedbackDao appFeedbackDao;
	
	/**
	 * @Description: 新增
	 * @param appFeedback 
	 * @return 受影响行数
	 * @author litianjian
	 * @date 2017年11月29日 下午03:19:08
	 */
	public int addAppFeedback(AppFeedback appFeedback) {
		return appFeedbackDao.insert(appFeedback);
	}
	
	/**
	 * @Description: 修改
	 * @param appFeedback 
	 * @return 受影响行数
	 * @author litianjian
	 * @date 2017年11月29日 下午03:19:08
	 */
	public int updateById(AppFeedback appFeedback) {
		return appFeedbackDao.updateById(appFeedback);
	}
	
	/**
	 * @Description: 删除
	 * @param id 主键
	 * @return 受影响行数
	 * @author litianjian
	 * @date 2017年11月29日 下午03:19:08
	 */
	public int deleteById(Long id) {
		return appFeedbackDao.deleteById(id);
	}
	
	/**
	 * @Description: 根据ID查询
	 * @param id 主键
	 * @return 
	 * @author litianjian
	 * @date 2017年11月29日 下午03:19:08
	 */
	public AppFeedback findById(Long id) {
		return appFeedbackDao.findById(id);
	}
	
	/**
	 * @Description: 分页查询
	 * @param appFeedbackVo 
	 * @param page 分页参数
	 * @return 
	 * @author litianjian
	 * @date 2017年11月29日 下午03:19:08
	 */
	public Pagination<AppFeedback> findByPagination(AppFeedbackVo appFeedbackVo, Page page) {
		Pagination<AppFeedback> paginationAppFeedback = appFeedbackDao.findByPagination(appFeedbackVo, page);
		return paginationAppFeedback;
	}
	
}