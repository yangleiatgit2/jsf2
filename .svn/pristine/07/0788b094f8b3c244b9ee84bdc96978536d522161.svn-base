package cn.uce.suc.bkg.biz.impl.datastatis;

import java.util.List;

import javax.annotation.Resource;

import org.springframework.stereotype.Service;

import cn.uce.base.page.Page;
import cn.uce.base.page.Pagination;
import cn.uce.suc.bkg.biz.datastatis.IFreightContrastReportBiz;
import cn.uce.suc.bkg.service.datastatis.FreightContrastReportService;
import cn.uce.suc.bkg.vo.datastatis.FreightContrastReportVo;

/**
 * 
 * @Description:  司机服务评价报表Biz实现类
 * @author xiesonglin
 * @date 2017年12月1日 下午12:19:51
 */
@Service("freightContrastReportBiz")
public class FreightContrastReportBiz implements IFreightContrastReportBiz {

	/**
	 * service注入
	 */
	@Resource
	private FreightContrastReportService freightContrastReportService;

	/**
	 * 根据分页查询货量差异数据统计报表 
	* <p>Title: findFreightContrastReportByPage</p> 
	* <p>Description: </p> 
	* @param freightContrastReportVo
	* @param page
	* @return 
	* @see cn.uce.suc.bkg.biz.infomon.IFreightContrastReportBiz#findFreightContrastReportByPage(cn.uce.suc.bkg.vo.infomon.FreightContrastReportVo, cn.uce.base.page.Page)
	 */
	public Pagination<FreightContrastReportVo> findFreightContrastReportByPage(
			FreightContrastReportVo freightContrastReportVo, Page page) {
		return this.freightContrastReportService.findFreightContrastReportByPage(freightContrastReportVo, page);
	}

	/**
	 * 根据条件查找出货量差异数据统计报表 并 用于数据导出 
	* <p>Title: findByExport</p> 
	* <p>Description: </p> 
	* @param freightContrastReportVo
	* @return 
	* @see cn.uce.suc.bkg.biz.infomon.IFreightContrastReportBiz#findByExport(cn.uce.suc.bkg.vo.infomon.FreightContrastReportVo)
	 */
	public List<FreightContrastReportVo> findByExport(FreightContrastReportVo freightContrastReportVo) {
		return this.freightContrastReportService.findByExport(freightContrastReportVo);
	}

}
