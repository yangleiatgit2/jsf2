package cn.uce.suc.common.service;

import javax.annotation.Resource;

import org.springframework.stereotype.Service;

import cn.uce.base.page.Page;
import cn.uce.base.page.Pagination;
import cn.uce.suc.common.dao.IOmgCmsOrgDetailDao;
import cn.uce.suc.common.entity.OmgCmsOrgDetail;
import cn.uce.suc.common.vo.OmgCmsOrgDetailVo;

/**
 * @Description: 乾坤机构详情Service类
 * @author liwei
 * @date 2017年10月15日 下午09:56:38
 */
@Service("omgCmsOrgDetailService")
public class OmgCmsOrgDetailService {
	
	/**
	 * 注入乾坤机构详情Dao
	 */
	@Resource
	private IOmgCmsOrgDetailDao omgCmsOrgDetailDao;
	
	
	/**
	 * @Description: 分页查询乾坤机构详情
	 * @param omgCmsOrgDetailVo 乾坤机构详情
	 * @param page 分页参数
	 * @return 乾坤机构详情
	 * @author liwei
	 * @date 2017年10月15日 下午09:56:38
	 */
	public Pagination<OmgCmsOrgDetail> findByPagination(OmgCmsOrgDetailVo omgCmsOrgDetailVo, Page page) {
		Pagination<OmgCmsOrgDetail> paginationOmgCmsOrgDetail = omgCmsOrgDetailDao.findByPagination(omgCmsOrgDetailVo, page);
		return paginationOmgCmsOrgDetail;
	}
	
}