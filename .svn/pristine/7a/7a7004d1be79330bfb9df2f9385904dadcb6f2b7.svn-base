	<select id="findSiteDepartPlan" parameterType="cn.uce.suc.bkg.vo.operation.DrivingPlanExecutedVo" resultType="cn.uce.suc.bkg.vo.operation.DrivingPlanExecutedVo">
	select t.way_point_code,t.way_point_name,t.driving_plan_code,a.driving_plan_name,
		t.id,t.forecast_end_time,t.actual_end_time,a.depart_time,a.arrival_time,
		t.forecast_volume,t.forecast_weight,t.actual_volume,t.actual_weight,t.actual_number,
		t.driver_actual_volume,t.driver_actual_weight,t.driver_actual_number,
		a.start_org_code,a.start_org_name,a.end_org_code,a.end_org_name,a.business_mode,
		a.status,t.plan_site_status,a.dirver_name,a.driver_phone,a.car_code,a.car_type,a.plate_number,a.next_station_code,a.next_station,
		t.station_eval_judge,t.station_eval_lever,t.station_appraise,t.appraise_time,t.plan_driver_prompt,t.plan_site_prompt
		FROM
			t_suc_bkg_dirving_plan_detail t
		LEFT OUTER JOIN t_suc_bkg_dirving_plan a ON t.driving_plan_code = a.driving_plan_code
		<where> 
			<if test="wayPointCode != null and  wayPointCode != ''">
				and t.way_point_code  = #{wayPointCode}
			</if>
			<if test="detailId != null and  detailId != ''">
				and t.id=#{detailId}
			</if>
			<if test="drivingPlanCode != null and  drivingPlanCode != ''">
				and a.driving_plan_code = #{drivingPlanCode}
			</if>
			<if test="stationEvalLever ==1">
				and t.station_eval_lever is null
			</if>
			<if test="stationEvalLever ==2">
				and t.station_eval_lever is not null
			</if>
			<if test="departTime != null and status != null and status!='' ">
			and STR_TO_DATE(a.depart_time,"%Y-%m-%d")=#{departTime} and a.status=#{status}
			</if>
		</where>
		order by t.create_time desc
	</select>
	<!-- 查询进港时 发车计划中的 最后一个网点 -->
	<select id="findLastStationByPlanCodeAndMode"  parameterType="cn.uce.suc.bkg.vo.operation.DirvingPlanDetailVo"  resultType="cn.uce.suc.bkg.vo.operation.DirvingPlanDetailVo" >
		select  
			<include refid="Base_Column_List" />
		from t_suc_bkg_dirving_plan_detail
		<where>
			driving_plan_code = #{drivingPlanCode} and sort =(
				SELECT  max(a.sort) FROM `t_suc_bkg_dirving_plan_detail` a where a.driving_plan_code == #{drivingPlanCode}
			)
		</where>
	</select>
	
	<!-- 根据网点编码/发车计划编码 查询发车计划明细 -->
	<select id="findByCode"  parameterType="cn.uce.suc.bkg.vo.operation.DirvingPlanDetailVo"  resultType="cn.uce.suc.bkg.vo.operation.DirvingPlanDetailVo">
		select <include refid="Base_Column_List" /> from t_suc_bkg_dirving_plan_detail
		<where>
			<if test="drivingPlanCode != null and  drivingPlanCode != ''">
				and driving_plan_code = #{drivingPlanCode}
			</if>
			<if test="wayPointCode != null and  wayPointCode != ''">
				and way_point_code = #{wayPointCode}
			</if>
		</where>
		order by sort desc
	</select>
	
	<!-- 根据网点编码/发车计划编码 查询发车计划明细    升序 -->
	<select id="findByCodeAsc"  parameterType="cn.uce.suc.bkg.vo.operation.DirvingPlanDetailVo"  resultType="cn.uce.suc.bkg.vo.operation.DirvingPlanDetailVo">
		select <include refid="Base_Column_List" /> from t_suc_bkg_dirving_plan_detail
		<where>
			<if test="drivingPlanCode != null and  drivingPlanCode != ''">
				and driving_plan_code = #{drivingPlanCode}
			</if>
			<if test="wayPointCode != null and  wayPointCode != ''">
				and way_point_code = #{wayPointCode}
			</if>
		</where>
		order by sort asc
	</select>
	
	<select id="findByDrivePlanCode"  parameterType="java.lang.String"  resultType="cn.uce.suc.bkg.entity.operation.DirvingPlanDetail" >
		select <include refid="Base_Column_List" /> from t_suc_bkg_dirving_plan_detail
		<where>
			driving_plan_code = #{drivingPlanCode} 
		</where>
	</select>
</mapper>