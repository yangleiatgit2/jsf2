package cn.uce.suc.common.cache;

import cn.uce.base.exception.BusinessException;
import cn.uce.core.cache.CacheSupport;
import cn.uce.suc.common.vo.CarPositioningVo;
/**
 * 
 * @Description:车辆位置缓存
 * @author liwei
 * @date 2017年11月13日 下午7:44:19
 */
public class CarPositioningCache extends CacheSupport<CarPositioningVo> {
	/**
	 * (非 Javadoc) 
	* <p>Title: doGet</p> 
	* <p>Description: </p> 
	* @param k
	* @return
	* @throws BusinessException 
	* @see cn.uce.core.cache.base.CacheCallBack#doGet(java.lang.Object)
	 */
	@Override
	public CarPositioningVo doGet(String k) throws BusinessException {
		return null;
	}
	/**
	 * (非 Javadoc) 
	* <p>Title: getCacheId</p> 
	* <p>Description: </p> 
	* @return 
	* @see cn.uce.core.cache.base.ICache#getCacheId()
	 */
	@Override
	public String getCacheId() {
		return this.getClass().getSimpleName();
	}
	/**
	 * 
	 * @Description:写入缓存方法
	 * @param key
	 * @param token
	 * @return
	 * @author lbnwxly
	 * @date 2017年10月10日 下午10:12:05
	 */
	public Boolean set(String key, CarPositioningVo carPositioningVo) {
		key = generate(key);
		int timeout = getTimeOut();
		if (timeout > 0) {
			return this.getStorage().set(key, carPositioningVo, getTimeOut());
		} else {
			return this.getStorage().set(key, carPositioningVo);
		}
	}
	/**
	 * 
	 * @Description:删除对应的key
	 * @param key
	 * @param token
	 * @return
	 * @author lbnwxly
	 * @date 2017年10月10日 下午10:12:05
	 */
	public void remove(String key) {
		key = generate(key);
		this.getStorage().remove(key);

	}
}
