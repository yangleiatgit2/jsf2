package cn.uce.suc.app.biz.impl;

import javax.annotation.Resource;

import org.springframework.stereotype.Service;

import cn.uce.base.page.Page;
import cn.uce.base.page.Pagination;
import cn.uce.suc.app.biz.IServiceevaluationBiz;
import cn.uce.suc.app.entity.Serviceevaluation;
import cn.uce.suc.app.vo.ServiceevaluationVo;
import cn.uce.suc.app.service.ServiceevaluationService;

/**
 * @Description: BIZ实现类
 * @author litianjian
 * @date 2017年11月28日 上午11:26:53
 */
@Service("tSucAppServiceevaluationBiz")
public class ServiceevaluationBiz implements IServiceevaluationBiz {
	
	/**
	 * 注入Service
	 */
	@Resource
	private ServiceevaluationService tSucAppServiceevaluationService;
	
	/**
	 * @Description: 新增
	 * @param tSucAppServiceevaluation
	 * @return 受影响行数
	 * @author litianjian
	 * @date 2017年11月28日 上午11:26:53
	 */
	public int addTSucAppServiceevaluation(Serviceevaluation tSucAppServiceevaluation) {
		return tSucAppServiceevaluationService.addTSucAppServiceevaluation(tSucAppServiceevaluation);
	}
	
	/**
	 * @Description: 修改
	 * @param tSucAppServiceevaluation 
	 * @return 受影响行数
	 * @author litianjian
	 * @date 2017年11月28日 上午11:26:53
	 */
	public int updateTSucAppServiceevaluation(Serviceevaluation tSucAppServiceevaluation) {
		return tSucAppServiceevaluationService.updateById(tSucAppServiceevaluation);
	}
	
	/**
	 * @Description: 删除
	 * @param id 主键
	 * @return 受影响行数
	 * @author litianjian
	 * @date 2017年11月28日 上午11:26:53
	 */
	public int deleteTSucAppServiceevaluation(Long id) {
		return tSucAppServiceevaluationService.deleteById(id);
	}
	
	/**
	 * @Description: 根据ID查询
	 * @param id 主键
	 * @return 
	 * @author litianjian
	 * @date 2017年11月28日 上午11:26:53
	 */
	@Override
	public Serviceevaluation findById(Long id) {
		return tSucAppServiceevaluationService.findById(id);
	}
	
	/**
	 * @Description: 分页查询
	 * @param tSucAppServiceevaluationVo 
	 * @param page 分页参数
	 * @return 
	 * @author litianjian
	 * @date 2017年11月28日 上午11:26:53
	 */
	public Pagination<Serviceevaluation> findByPagination(ServiceevaluationVo tSucAppServiceevaluationVo, Page page) {
		Pagination<Serviceevaluation> pagination = tSucAppServiceevaluationService.findByPagination(tSucAppServiceevaluationVo, page);
		return pagination;
	}

	@Override
	public Serviceevaluation findScoreforDriver(String mobliePhone) {
		// TODO Auto-generated method stub
		Serviceevaluation tSucAppServiceevaluation = tSucAppServiceevaluationService.findScoreforDriver(mobliePhone);
		return tSucAppServiceevaluation;
	}

}