package cn.uce.suc.app.control;

import java.util.Map;

import javax.annotation.Resource;

import org.springframework.stereotype.Controller;
import org.springframework.web.bind.annotation.GetMapping;
import org.springframework.web.bind.annotation.RequestMapping;
import org.springframework.web.bind.annotation.ResponseBody;

import cn.uce.suc.app.biz.impl.AppFeedbackBiz;
import cn.uce.suc.app.entity.AppFeedback;
import cn.uce.suc.common.util.BaseAppController;
/**
 * 
 * @Description: 建议反馈
 * @author litianjian
 * @date 2017年12月7日 下午4:03:13
 */
@Controller
@RequestMapping("/app/appFeedBackBizController")
public class AppFeedBackBizController  extends BaseAppController{
		/**
		 * 注入
		 */
		@Resource
		AppFeedbackBiz appFeedbackBiz;
		
		
		/**
		 * 
		 * @Description:添加建议
		 * @param appFeedback
		 * @return
		 * @author
		 * @date 2017年12月7日 下午4:03:36
		 */
		@GetMapping(value = "addFeedBack")
		@ResponseBody
		public Map<String, Object> addFeedBack(AppFeedback appFeedback){
			appFeedbackBiz.addAppFeedback(appFeedback);
			return returnSuccess();

		}
}
