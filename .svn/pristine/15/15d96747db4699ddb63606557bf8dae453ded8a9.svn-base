	<select id="findByPagination" parameterType="cn.uce.suc.bkg.vo.operation.FreightOrderVo"
		resultType="cn.uce.suc.bkg.entity.operation.FreightOrder">
		select
		<include refid="Base_Column_List" />
		from t_suc_bkg_freight_order
		<include refid="Base_Column_Where" />
		
		order by create_time  DESC
	</select>
    <select id="findByCondtion" parameterType="cn.uce.suc.bkg.vo.operation.FreightOrderVo"
		resultType="cn.uce.suc.bkg.entity.operation.FreightOrder">
		select
		<include refid="Base_Column_List" />
		from t_suc_bkg_freight_order
		<include refid="Base_Column_Where" />
	</select>
	<!-- 根据OrderCode进展更新  begin-->
	<update id="updateByOrderCode" parameterType="cn.uce.suc.bkg.entity.operation.FreightOrder">
			UPDATE t_suc_bkg_freight_order
			<set>
			order_combin_code = #{orderCombinCode},
			order_combin_name = #{orderCombinName}
				
			</set>
			WHERE order_code = #{orderCode}
	</update>
	<!-- 根据OrderCode进展更新  end-->
	<!-- 根据组合查询订单 -->
	<select id="findByCombine" parameterType="cn.uce.suc.bkg.vo.operation.FreightOrderVo" resultType="cn.uce.suc.bkg.vo.operation.FreightOrderVo">
		SELECT fo.*,oe.depart_limit,oe.port_limit
		FROM t_suc_bkg_freight_order fo
		<if test="businessMode != null and businessMode != '' ">
			LEFT JOIN t_suc_comm_org_extend oe 
			<!-- 进港 -->
			<if test='businessMode == "1"'>
				ON fo.end_station_code = oe.base_org_code 
			</if>
			<!-- 出港 -->
			<if test='businessMode == "2"'>
				ON fo.begin_station_code = oe.base_org_code 
			</if>
		</if>
		<where>
			<if test="orderStatus != null and orderStatus != ''">
				AND fo.order_status = #{orderStatus}
			</if>
			<if test="businessMode != null and businessMode != ''">
				AND fo.business_mode = #{businessMode}
			</if>
			<if test="orderCombinCode != null and orderCombinCode != ''">
				AND fo.order_combin_code = #{orderCombinCode}
			</if>
		
		</where>
	</select>
	
	<select id="findByVo" parameterType="cn.uce.suc.bkg.vo.operation.FreightOrderVo" resultType="cn.uce.suc.bkg.vo.operation.FreightOrderVo">
		SELECT fo.* FROM t_suc_bkg_freight_order fo
	
		<where>
			<if test="demandSiteCode != null and demandSiteCode != ''">
			AND demand_site_code = #{demandSiteCode}
			</if>
			<if test="executeStartTime != null">  
			  AND execute_start_time &gt; date (#{executeStartTime})
			</if>  
			<if test="executeEndTime != null">  
			  AND execute_end_time &lt; date(#{executeEndTime}) 
			</if> 
		</where>
	</select>

	<!-- 出港-->
	<select id="findEndStationCode" resultType="java.lang.String" parameterType="cn.uce.suc.bkg.vo.operation.FreightOrderVo">
	select end_station_code from t_suc_bkg_freight_order where business_mode = #{businessMode} and demand_type = #{demandType} and (order_combin_code is null or order_combin_code='') and order_start_time between #{beginTime} and #{endTime} GROUP BY end_station_code;
	</select>
	<!--  进港-->
	<select id="findbeginStationCode" resultType="java.lang.String" parameterType="cn.uce.suc.bkg.vo.operation.FreightOrderVo">
	select begin_station_code from t_suc_bkg_freight_order where business_mode = #{businessMode} and demand_type = #{demandType} and (order_combin_code is null or order_combin_code='') and order_start_time between #{beginTime} and #{endTime} GROUP BY begin_station_code;
	</select>
	
	
	<select id="findByOderNoTimeDesc" parameterType="cn.uce.suc.bkg.vo.operation.FreightOrderVo" resultType="cn.uce.suc.bkg.vo.operation.FreightOrderVo">
		SELECT fo.* FROM t_suc_bkg_freight_order fo
	
		<where>
			<if test="orderCombinCode != null and orderCombinCode != ''">
