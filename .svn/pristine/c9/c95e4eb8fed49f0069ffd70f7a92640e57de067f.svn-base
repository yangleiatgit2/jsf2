package cn.uce.suc.bkg.biz.impl.datastatis;

import java.util.List;

import javax.annotation.Resource;

import org.springframework.stereotype.Service;

import cn.uce.base.page.Page;
import cn.uce.base.page.Pagination;
import cn.uce.suc.bkg.biz.datastatis.IDriverEvaluateReportBiz;
import cn.uce.suc.bkg.service.datastatis.DriverEvaluateReportService;
import cn.uce.suc.bkg.vo.datastatis.DriverEvaluateReportVo;

/**
 * 
 * @Description:  司机服务评价报表Biz实现类
 * @author xiesonglin
 * @date 2017年12月1日 下午12:19:51
 */
@Service("driverEvaluateReportBiz")
public class DriverEvaluateReportBiz implements IDriverEvaluateReportBiz {

	/**
	 * service注入
	 */
	@Resource
	private DriverEvaluateReportService driverEvaluateReportService;

	/**
	 * 根据分页查询司机服务评价报表
	 * <p>
	 * Title: findDriverEvaluateReportByPage
	 * </p>
	 * <p>
	 * Description:
	 * </p>
	 * 
	 * @param driverEvaluateReportVo
	 * @param page
	 * @return
	 * @see cn.uce.suc.bkg.biz.infomon.IDriverEvaluateReportBiz#findDriverEvaluateReportByPage(cn.uce.suc.bkg.vo.infomon.DriverEvaluateReportVo,
	 *      cn.uce.base.page.Page)
	 */
	public Pagination<DriverEvaluateReportVo> findDriverEvaluateReportByPage(
			DriverEvaluateReportVo driverEvaluateReportVo, Page page) {
		return this.driverEvaluateReportService.findDriverEvaluateReportByPage(driverEvaluateReportVo, page);
	}

	/**
	 * 用于导出数据 
	* <p>Title: findByExport</p> 
	* <p>Description: </p> 
	* @param driverEvaluateReportVo
	* @return 
	* @see cn.uce.suc.bkg.biz.infomon.IDriverEvaluateReportBiz#findByExport(cn.uce.suc.bkg.vo.infomon.DriverEvaluateReportVo)
	 */
	public List<DriverEvaluateReportVo> findByExport(DriverEvaluateReportVo driverEvaluateReportVo) {
		return this.driverEvaluateReportService.findByExport(driverEvaluateReportVo);
	}

}
