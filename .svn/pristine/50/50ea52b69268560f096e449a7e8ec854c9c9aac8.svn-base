package cn.uce.suc.bkg.exception;

import cn.uce.base.exception.BusinessException;
/**
 * 
 * @Description: 承运商平台异常类
 * @author liwei
 * @date 2017年10月18日 下午9:40:52
 */
public class BkgPriceRuleException extends BusinessException{
	//序列化
	private static final long serialVersionUID = -4883689301600546900L;
	
	/**
	 * 
	 * @param code
	 */
	public BkgPriceRuleException(String code){
		super(code);
		super.code = code;
	}
	/**
	 * 
	 * @param code
	 * @param msg
	 * @param cause
	 */
	public BkgPriceRuleException(String code, String msg, Throwable cause) {
		super(code, msg, cause);
	}

	/**
	 * 
	 * @param code
	 * @param msg
	 */
	public BkgPriceRuleException(String code, String msg) {
		super(code, msg);
	}
}
