package cn.uce.suc.app.entity;

import cn.uce.base.entity.BaseEntity;
import cn.uce.core.db.annotion.Table;
import java.util.Date;

/**
 * @Description: 验证码Entity类
 * @author liwei
 * @date 2017年11月05日 下午08:45:38
 */
@Table("t_suc_app_captcha")
public class Captcha extends BaseEntity {
	/**
	 * serialVersionUID
	 */
	private static final long serialVersionUID = 1L;
	
    /** 
	 * 手机号 
	 */
	private String mobilePhone;
    /** 
	 * 内容 
	 */
	private String content;
    /** 
	 * 次数 
	 */
	private Integer count;
    /** 
	 * 失效时间 
	 */
	private Date deadTime;
    /** 
	 * 平台类型 
	 */
	private String userType;
    /** 
	 * 备注 
	 */
	private String remark;
    /** 
	 * 删除标示 
	 */
	private Boolean deleteFlag;
    /** 
	 * 版本号 
	 */
	private Integer version;
    /** 
	 * 创建人 
	 */
	private String createOrg;
    /** 
	 * 更新组织 
	 */
	private String updateOrg;
	
	
	/**
	 * 获取 手机号
	 */
	public String getMobilePhone() {
		return mobilePhone;
	}
	
	/**
	 * 设置 手机号
	 */
	public void setMobilePhone(String mobilePhone) {
		this.mobilePhone = mobilePhone;
	}
	
	/**
	 * 获取 内容
	 */
	public String getContent() {
		return content;
	}
	
	/**
	 * 设置 内容
	 */
	public void setContent(String content) {
		this.content = content;
	}
	
	/**
	 * 获取 次数
	 */
	public Integer getCount() {
		return count;
	}
	
	/**
	 * 设置 次数
	 */
	public void setCount(Integer count) {
		this.count = count;
	}
	
	/**
	 * 获取 失效时间
	 */
	public Date getDeadTime() {
		return deadTime;
	}
	
	/**
	 * 设置 失效时间
	 */
	public void setDeadTime(Date deadTime) {
		this.deadTime = deadTime;
	}
	
	/**
	 * 获取 平台类型
	 */
	public String getUserType() {
		return userType;
	}
	
	/**
	 * 设置 平台类型
	 */
	public void setUserType(String userType) {
		this.userType = userType;
	}
	
	/**
	 * 获取 备注
	 */
	public String getRemark() {
		return remark;
	}
	
	/**
	 * 设置 备注
	 */
	public void setRemark(String remark) {
		this.remark = remark;
	}
	
	/**
	 * 获取 删除标示
	 */
	public Boolean getDeleteFlag() {
		return deleteFlag;
	}
	
	/**
	 * 设置 删除标示
	 */
	public void setDeleteFlag(Boolean deleteFlag) {
		this.deleteFlag = deleteFlag;
	}
	
	/**
	 * 获取 版本号
	 */
	public Integer getVersion() {
		return version;
	}
	
	/**
	 * 设置 版本号
	 */
	public void setVersion(Integer version) {
		this.version = version;
	}
	
	/**
	 * 获取 创建人
	 */
	public String getCreateOrg() {
		return createOrg;
	}
	
	/**
	 * 设置 创建人
	 */
	public void setCreateOrg(String createOrg) {
		this.createOrg = createOrg;
	}
	
	/**
	 * 获取 更新组织
	 */
	public String getUpdateOrg() {
		return updateOrg;
	}
	
	/**
	 * 设置 更新组织
	 */
	public void setUpdateOrg(String updateOrg) {
		this.updateOrg = updateOrg;
	}
	
	
}