package cn.uce.suc.app.control;

import java.util.Map;

import javax.annotation.Resource;

import org.springframework.web.bind.annotation.RequestMapping;
import org.springframework.web.bind.annotation.RequestMethod;
import org.springframework.web.bind.annotation.ResponseBody;
import org.springframework.web.bind.annotation.RestController;

import cn.uce.base.page.Page;
import cn.uce.base.page.Pagination;
import cn.uce.suc.common.biz.IOmgCmsOrgBiz;
import cn.uce.suc.common.util.BaseAppController;
import cn.uce.suc.common.vo.ComboOrgVo;
import cn.uce.suc.common.vo.OmgCmsOrgVo;
/**
 * 
 * @Description: 乾坤机构表接口
 * @author zhandesheng
 * @date 2017年10月25日 下午5:25:02
 */
@RestController
@RequestMapping("/app/omgCmsOrg")
public class AppOmgCmsOrgController extends BaseAppController{
	/**
	 * 注入乾坤机构biz
	 * 
	 */
	@Resource
	private IOmgCmsOrgBiz omgCmsOrgBiz;

	/**
	 * 
	 * @Description: 乾坤机构数据分页查询
	 * @param omgCmsOrgVo
	 * @param currentPage
	 * @param pageSize
	 * @return
	 * @author zhandesheng
	 * @date 2017年10月26日 下午3:14:38
	 */
	@RequestMapping(value = "findByOmgCmsOrg",method=RequestMethod.GET)
	@ResponseBody
	public  Map<String, Object> findAllOmgCmsOrg(OmgCmsOrgVo omgCmsOrgVo, int currentPage,int pageSize){
		Page page = new Page();
		page.setCurrentPage(currentPage);
		page.setPageSize(pageSize);
		Pagination<ComboOrgVo> findByCombox = omgCmsOrgBiz.findByCombox(omgCmsOrgVo, page);
		return returnSuccess(findByCombox);
	}
	
	
	
	
	
	
}
