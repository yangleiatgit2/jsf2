<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC 
	"-//mybatis.org//DTD Mapper 3.0//EN" 
	"http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="cn.uce.suc.bkg.dao.operation.IIaConfDao">
	
	<sql id="Base_Column_List">
			id,org_code,org_name,
			full_load_rate,arrival_time_threshold,
			max_parallel_times,max_parallel_volume,capacity_slack,utf1,
			utf2,utf3,remark,
			version,delete_flag,create_emp,
			create_time,create_org,update_emp,
			update_time,update_org
	</sql>
	
	<sql id="Base_Column_Where">
	<where>
	    <if test="id != null">
		and id = #{id}
		</if>
	    <if test="orgCode != null">
		and org_code = #{orgCode}
		</if>
	    <if test="orgName != null">
		and org_name = #{orgName}
		</if>
	    <if test="fullLoadRate != null">
		and full_load_rate = #{fullLoadRate}
		</if>
	    <if test="arrivalTimeThreshold != null">
		and arrival_time_threshold = #{arrivalTimeThreshold}
		</if>
	    <if test="maxParallelTimes != null">
	    and max_parallel_times = #{maxParallelTimes}
	    </if>
	    <if test="maxParallelVolume != null">
	    and max_parallel_volume = #{maxParallelVolume}
	    </if>
	    <if test="capacitySlack != null">
	    and capacity_slack = #{capacitySlack}
	    </if>
	    <if test="utf1 != null">
		and utf1 = #{utf1}
		</if>
	    <if test="utf2 != null">
		and utf2 = #{utf2}
		</if>
	    <if test="utf3 != null">
		and utf3 = #{utf3}
		</if>
	    <if test="remark != null">
		and remark = #{remark}
		</if>
	    <if test="version != null">
		and version = #{version}
		</if>
	    <if test="deleteFlag != null">
		and delete_flag = #{deleteFlag}
		</if>
	    <if test="createEmp != null">
		and create_emp = #{createEmp}
		</if>
	    <if test="createTime != null">
		and create_time = #{createTime}
		</if>
	    <if test="createOrg != null">
		and create_org = #{createOrg}
		</if>
	    <if test="updateEmp != null">
		and update_emp = #{updateEmp}
		</if>
	    <if test="updateTime != null">
		and update_time = #{updateTime}
		</if>
	    <if test="updateOrg != null">
		and update_org = #{updateOrg}
		</if>
	</where>
	</sql>
	
	<update id="updateById" parameterType="cn.uce.suc.bkg.entity.operation.IaConf">
	UPDATE t_suc_bkg_ia_conf
	<set>
		<if test="orgCode != null">
		org_code = #{orgCode},
		</if>
		<if test="orgName != null">
		org_name = #{orgName},
		</if>
		<if test="fullLoadRate != null">
		full_load_rate = #{fullLoadRate},
		</if>
		<if test="arrivalTimeThreshold != null">
		arrival_time_threshold = #{arrivalTimeThreshold},
		</if>
		<if test="maxParallelTimes != null">
	    max_parallel_times = #{maxParallelTimes},
	    </if>
	    <if test="maxParallelVolume != null">
	    max_parallel_volume = #{maxParallelVolume},
	    </if>
	    <if test="capacitySlack != null">
	    capacity_slack = #{capacitySlack},
	    </if>
		<if test="utf1 != null">
		utf1 = #{utf1},
		</if>
		<if test="utf2 != null">
		utf2 = #{utf2},
		</if>
		<if test="utf3 != null">
		utf3 = #{utf3},
		</if>
		<if test="remark != null">
		remark = #{remark},
		</if>
		<if test="version != null">
		version = ifnull(version,0) + 1,
		</if>
		<if test="deleteFlag != null">
		delete_flag = #{deleteFlag},
		</if>
		<if test="createEmp != null">
		create_emp = #{createEmp},
		</if>
		<if test="createTime != null">
		create_time = #{createTime},
		</if>
		<if test="createOrg != null">
		create_org = #{createOrg},
		</if>
		<if test="updateEmp != null">
		update_emp = #{updateEmp},
		</if>
		<if test="updateTime != null">
		update_time = #{updateTime},
		</if>
		<if test="updateOrg != null">
		update_org = #{updateOrg},
		</if>
	</set>
	WHERE id = #{id}
		<if test="version != null">
			AND version = #{version}
		</if>
	</update>
	
	
	<select id="findByPagination" parameterType="cn.uce.suc.bkg.vo.operation.IaConfVo" resultType="cn.uce.suc.bkg.vo.operation.IaConfVo">
		select <include refid="Base_Column_List" /> 
		from t_suc_bkg_ia_conf
		<where>
			delete_flag != '1'
			<if test="fullLoadRate != null">
				AND full_load_rate = #{fullLoadRate}
			</if>
			<if test="arrivalTimeThreshold != null">
				AND arrival_time_threshold = #{arrivalTimeThreshold}
			</if>
			<if test="orgCode != null and orgCode != ''">
				AND org_code = #{orgCode}
			</if>
			<if test="id != null">
				AND id = #{id}
			</if>
		</where>
		ORDER BY create_time DESC
	</select>
	
	<select id="findByCondition" parameterType="cn.uce.suc.bkg.vo.operation.IaConfVo" resultType="cn.uce.suc.bkg.vo.operation.IaConfVo">
		SELECT <include refid="Base_Column_List" /> 
		FROM t_suc_bkg_ia_conf
		<where>
			delete_flag != '1'
			<if test="fullLoadRate != null">
				AND full_load_rate = #{fullLoadRate}
			</if>
			<if test="arrivalTimeThreshold != null">
				AND arrival_time_threshold = #{arrivalTimeThreshold}
			</if>
			<if test="orgCode != null and orgCode != ''">
				AND org_code = #{orgCode}
			</if>
			<if test="id != null">
				AND id = #{id}
			</if>
		</where>
	</select>
	
</mapper>