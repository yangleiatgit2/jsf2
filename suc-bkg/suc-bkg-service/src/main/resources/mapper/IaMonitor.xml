<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC 
	"-//mybatis.org//DTD Mapper 3.0//EN" 
	"http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="cn.uce.suc.bkg.dao.operation.IIaMonitorDao">
	
	<sql id="Base_Column_List">
			id,task_code,org_code,
			org_name,combine_code,combine_type,
			demand_type,business_mode,status,
			error_msg,execute_time,utf1,
			utf2,utf3,delete_flag,
			version,remark,create_org,
			create_emp,create_time,update_org,
			update_emp,update_time
	</sql>
	
	<sql id="Base_Column_Where">
	<where>
	    <if test="id != null">
		and id = #{id}
		</if>
	    <if test="taskCode != null">
		and task_code = #{taskCode}
		</if>
	    <if test="orgCode != null">
		and org_code = #{orgCode}
		</if>
	    <if test="orgName != null">
		and org_name = #{orgName}
		</if>
	    <if test="combineCode != null">
		and combine_code = #{combineCode}
		</if>
	    <if test="combineType != null">
		and combine_type = #{combineType}
		</if>
	    <if test="demandType != null">
		and demand_type = #{demandType}
		</if>
	    <if test="businessMode != null">
		and business_mode = #{businessMode}
		</if>
	    <if test="status != null">
		and status = #{status}
		</if>
	    <if test="errorMsg != null">
		and error_msg = #{errorMsg}
		</if>
	    <if test="executeTime != null">
		and execute_time = #{executeTime}
		</if>
	    <if test="utf1 != null">
		and utf1 = #{utf1}
		</if>
	    <if test="utf2 != null">
		and utf2 = #{utf2}
		</if>
	    <if test="utf3 != null">
		and utf3 = #{utf3}
		</if>
	    <if test="deleteFlag != null">
		and delete_flag = #{deleteFlag}
		</if>
	    <if test="version != null">
		and version = #{version}
		</if>
	    <if test="remark != null">
		and remark = #{remark}
		</if>
	    <if test="createOrg != null">
		and create_org = #{createOrg}
		</if>
	    <if test="createEmp != null">
		and create_emp = #{createEmp}
		</if>
	    <if test="createTime != null">
		and create_time = #{createTime}
		</if>
	    <if test="updateOrg != null">
		and update_org = #{updateOrg}
		</if>
	    <if test="updateEmp != null">
		and update_emp = #{updateEmp}
		</if>
	    <if test="updateTime != null">
		and update_time = #{updateTime}
		</if>
	</where>
	</sql>
	
	<update id="updateById" parameterType="cn.uce.suc.bkg.entity.operation.IaMonitor">
	UPDATE t_suc_bkg_ia_monitor
	<set>
		<if test="taskCode != null">
		task_code = #{taskCode},
		</if>
		<if test="orgCode != null">
		org_code = #{orgCode},
		</if>
		<if test="orgName != null">
		org_name = #{orgName},
		</if>
		<if test="combineCode != null">
		combine_code = #{combineCode},
		</if>
		<if test="combineType != null">
		combine_type = #{combineType},
		</if>
		<if test="demandType != null">
		demand_type = #{demandType},
		</if>
		<if test="businessMode != null">
		business_mode = #{businessMode},
		</if>
		<if test="status != null">
		status = #{status},
		</if>
		<if test="errorMsg != null">
		error_msg = #{errorMsg},
		</if>
		<if test="executeTime != null">
		execute_time = #{executeTime},
		</if>
		<if test="utf1 != null">
		utf1 = #{utf1},
		</if>
		<if test="utf2 != null">
		utf2 = #{utf2},
		</if>
		<if test="utf3 != null">
		utf3 = #{utf3},
		</if>
		<if test="deleteFlag != null">
		delete_flag = #{deleteFlag},
		</if>
		<if test="version != null">
		version = #{version},
		</if>
		<if test="remark != null">
		remark = #{remark},
		</if>
		<if test="createOrg != null">
		create_org = #{createOrg},
		</if>
		<if test="createEmp != null">
		create_emp = #{createEmp},
		</if>
		<if test="createTime != null">
		create_time = #{createTime},
		</if>
		<if test="updateOrg != null">
		update_org = #{updateOrg},
		</if>
		<if test="updateEmp != null">
		update_emp = #{updateEmp},
		</if>
		<if test="updateTime != null">
		update_time = #{updateTime},
		</if>
	</set>
	WHERE id = #{id}
	</update>
	
	<select id="findByPagination" parameterType="cn.uce.suc.bkg.vo.operation.IaMonitorVo" resultType="cn.uce.suc.bkg.entity.operation.IaMonitor">
		select <include refid="Base_Column_List" /> from t_suc_bkg_ia_monitor
		<where>
		    <if test="combineCode != null and combineCode != ''">
				and combine_code LIKE CONCAT('%',#{combineCode},'%')
			</if>
		    <if test="combineType != null and combineType != ''">
				and combine_type = #{combineType}
			</if>
		    <if test="demandType != null and demandType != ''">
				and demand_type = #{demandType}
			</if>
		    <if test="businessMode != null and businessMode != ''">
				and business_mode = #{businessMode}
			</if>
		    <if test="status != null and status != ''">
				and status = #{status}
			</if>
			<if test="startTime != null">
				and execute_time >= #{startTime} 
			</if>
			<if test="endTime != null">
				and execute_time <![CDATA[<=]]> #{endTime}
			</if>
			<if test="orgCode != null and orgCode != ''">
				and org_code = #{orgCode}
			</if>
		    <if test="orgName != null and orgName != ''">
				and org_name = #{orgName}
			</if>
		</where>
		ORDER BY create_time DESC
	</select>
	
	<!-- 根据组合编号查询算法任务 -->
	<select id="findByCombineCode" parameterType="java.lang.String" resultType="cn.uce.suc.bkg.vo.operation.IaMonitorVo">
		SELECT * FROM t_suc_bkg_ia_monitor WHERE combine_code = #{combineCode}
	</select>
</mapper>